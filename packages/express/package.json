  "devDependencies": {
    "@types/express": "^4.17.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "test": "pnpm --filter @agent-tokens/core build && vitest run",
    "lint": "node -e \"process.exit(0)\"",
    "fmt": "node -e \"process.exit(0)\""
  }
