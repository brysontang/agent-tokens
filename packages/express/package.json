{
  "name": "@agent-tokens/express",
  "version": "0.1.0",
  "description": "Express middleware for Agent Tokens validation and intent enforcement.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brysontang/agent-tokens.git"
  },
  "homepage": "https://agenttokens.org",
  "bugs": "https://github.com/brysontang/agent-tokens/issues",
  "keywords": ["express", "middleware", "agent", "ai", "security"],
  "publishConfig": { "access": "public" },
  "engines": { "node": ">=18" },
